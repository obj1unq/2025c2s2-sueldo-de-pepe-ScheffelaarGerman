import pepe.*   // importa todos tus objetos

describe "Sueldo de Pepe" {

  method initialize() {
    pepe.categoria = cadete
    pepe.bonoResultado = montoFijo
    pepe.bonoPresentismo = normal
    pepe.faltas = 3
  }

  test "Caso 1: cadete + monto fijo + normal con 3 faltas = 20800" {
    assert.equals(20800, pepe.sueldo())
  }

}
